<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="521.0" prefWidth="696.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.Calendar.CalendarActivityTable">
   <children>
      <TableView fx:id="activityTable" layoutX="48.0" layoutY="52.0" prefHeight="400.0" prefWidth="600.0">
         <columns>
            <TableColumn prefWidth="50.0" text="ID">
               <cellValueFactory>
                  <PropertyValueFactory property="id" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="262.0" text="Title">
               <cellValueFactory>
                  <PropertyValueFactory property="title" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="186.0" text="Date">
               <cellValueFactory>
                  <PropertyValueFactory property="date" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="101.0" text="Priority">
               <cellValueFactory>
                  <PropertyValueFactory property="priority" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
      <Button id="Remove Button" layoutX="319.0" layoutY="473.0" mnemonicParsing="false" text="Remove" onAction="#removeActivity" />
      <Label layoutX="308.0" layoutY="21.0" text="Activities Table" />
   </children>
</AnchorPane>
