<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.HomeController.HomeController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Button id="timerButton" mnemonicParsing="false" onAction="#handleTimerButtonClick" text="Timer">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button id="calendarButton" mnemonicParsing="false" onAction="#handleCalendarButtonClick" text="Calendar">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Settings">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
         </children></VBox>
      <VBox alignment="CENTER" layoutX="500.0" prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Button id="profileButton" mnemonicParsing="false" onAction="#handleProfileButtonClick" text="Profile" />
            <Button mnemonicParsing="false" text="24/7 Support" />
         </children></VBox>
      <VBox layoutX="100.0" prefHeight="400.0" prefWidth="394.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Hello, username!">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Daily Quote: &quot;???&quot;">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Text>
            <TableView prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Tasks" />
                <TableColumn prefWidth="75.0" text="Progress" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
