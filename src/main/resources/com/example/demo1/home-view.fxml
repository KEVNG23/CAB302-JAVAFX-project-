<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.Controller.HomeController">
   <children>
      <VBox prefHeight="400.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" text="Timer">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button id="calendarButton" mnemonicParsing="false" onAction="#handleCalendarButtonClick" text="Calendar">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Settings">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
      <VBox layoutX="500.0" prefHeight="400.0" prefWidth="100.0" />
   </children>
</AnchorPane>
